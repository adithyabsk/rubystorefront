<p id="notice"><%= notice %></p>

<h1>Restricted Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @restricted_requests.each do |restricted_request| %>
      <tr>
        <td><%= link_to 'Show', restricted_request %></td>
        <td><%= link_to 'Edit', edit_restricted_request_path(restricted_request) %></td>
        <td><%= link_to 'Destroy', restricted_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Restricted Request', new_restricted_request_path %>
